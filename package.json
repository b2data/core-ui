    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-refresh": "^0.4.5",
    "marked": "^15.0.0",
    "notistack": "^3.0.1",
    "prettier": "^3.0.0",
    "ramda": "^0.30.1",
    "react": "^18.2.15",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
