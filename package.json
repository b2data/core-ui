  "scripts": {
    "start": "vite",
    "clean": "vite clean",
    "compile": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && cpx \"src/**/*.css\" lib",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "lint:fix": "eslint --ext .js,.ts,.tsx src --fix",
    "test": "tsc --noEmit --project .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|tsx|json)\"",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006 --docs",
    "storybook:ssl": "storybook dev -p 6006 --docs --https --ssl-cert=./localhost.pem --ssl-key=localhost-key.pem",
    "storybook:build": "storybook build",
    "storybook:build-docs": "storybook build --docs",
    "storybook:prepare": "npm run storybook:build-docs && touch ./storybook-static/.nojekyll",
    "storybook:deploy": "npm run storybook:prepare && gh-pages -d storybook-static -t true"
  },
