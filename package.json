{
  "name": "@b2data/core-ui",
  "version": "1.0.1",
  "description": "",
  "files": [
    "lib"
  ],
  "main": "lib/index.ts",
  "scripts": {
    "start": "vite",
    "babel": "babel --copy-files src/ -d lib/ --ignore 'src/assets/','src/main.ts','src/*d.ts','src/**/**/*.stories.@(tsx|ts)'",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "test:types": "tsc --noEmit --project .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|tsx|json)\"",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "storybook:docs": "storybook dev --docs -p 6006",
    "storybook:build": "storybook build",
    "storybook:build-docs": "storybook build --docs",
    "storybook:prepare": "npm run storybook:build-docs && touch ./storybook-static/.nojekyll",
    "storybook:deploy": "npm run storybook:prepare && gh-pages -d storybook-static -t true"
  },
  "author": "Nikolai Turov",
  "license": "MIT",
  "homepage": "https://b2data.github.io/core-ui",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "@mui/x-date-pickers": "^6.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling": "^1.3.5",
    "@storybook/react": "^7.2.1",
    "@storybook/react-vite": "^7.2.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-cli": "^6.26.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.0",
    "prettier": "^3.0.0",
    "storybook": "^7.2.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
