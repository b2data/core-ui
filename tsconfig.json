{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noImplicitAny": true,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    // "sourceMap": true,
    "preserveSymlinks": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@mui/x-data-grid": [
        "src/mui-x/grid"
      ],
      "@mui/x-data-grid/*": [
        "src/mui-x/grid/*"
      ],
      "@mui/x-data-grid-pro": [
        "src/mui-x/pro"
      ],
      "@mui/x-data-grid-pro/*": [
        "src/mui-x/pro/*"
      ],
      "@mui/x-data-grid-premium": [
        "src/mui-x/premium"
      ],
      "@mui/x-data-grid-premium/*": [
        "src/mui-x/premium/*"
      ],
      "@mui/x-internals": [
        "./src/mui-x/internals"
      ],
      "@mui/x-internals/*": [
        "./src/mui-x/internals/*"
      ],
      "@mui/x-date-pickers": [
        "src/mui-x/pickers"
      ],
      "@mui/x-date-pickers/*": [
        "src/mui-x/pickers/*"
      ],
    }
  },
  "include": [
    "src",
    ".storybook",
  ],
  "exclude": [
    "node_modules",
  ]
}